{
  "name": "note",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "title": {
      "type": "string",
      "required": true
    },
    "tags": {
      "type": [
        "string"
      ]
    },
    "body": {
      "type": "string",
      "dataType": "MEDIUMTEXT"
    },
    "dateCreated": {
      "type": "date",
      "required": true,
      "mysql": {
        "default":"CURRENT_TIMESTAMP"
      }
    },
    "dateModified": {
      "type": "date",
      "mysql": {
        "default":"CURRENT_TIMESTAMP"
      }
    },
    "author": {
      "type": "string",
      "required": true
    },
    "id": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
